{
    "root": true,
    "env": {
        "es6": false,
        "node": true,
        "jest": true
    },
    "extends": [
        "eslint:recommended",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
    ],
    "rules": {
        "quotes": [
            2, "double", {
                "avoidEscape": true
            }
        ]
    }
}
